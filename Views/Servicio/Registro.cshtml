@model Ticket

<div class="container">
    <h1 class="text-center">Registrar Servicio</h1>
    <form method="POST" action="">
        <div class="form-group">
            <label for="">Cliente: </label>
            <input type="search" placeholder="Cliente" class="form-control">
            <select asp-for="ClienteId" asp-items="ViewBag.clientes" class="form-control"></select>
        </div>
        <div class="form-group">
            <label for="">Fecha: </label>
            <input type="date" name="" id="" class="form-control" disabled>
        </div>
        <div class="form-group text-center">
            <div class="row">
                <div class="col">
                    <input type="submit" value="Registrar" class="btn btn-success">
                </div>
                <div class="col">
                    <input type="submit" value="Cancelar" class="btn btn-warning">
                </div>
            </div>
        </div>
    </form>

    @if(ViewBag.axcel != null)
    {
        <h2>@ViewBag.axcel</h2>
    }
</div>

